.section
    box-sizing border-box
    width 1024px
    min-height 78vh;    
    margin:0 auto;   
    display: flex;

.row
    width:500px;
    height:500px;
    border:40px solid #FFFFFF;
    margin-top 100px
    margin-left 50px
    display flex
    flex-wrap wrap
    align-items center
    align-content space-around
    justify-content space-evenly
    background-color: #1F495E;    


.chislo
    flex:1 1 100%;
    height:35%;    
    text-align: center;
   

.span-chislo{    
    display: inline-block;
    margin-top: 70px;
    color:#FFFFFF;
    font-family: 'Arimo', sans-serif;
    font-weight: 700;
    font-size: 110px ;    
}
.container-nomer{
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
}
.nomer{
    margin-top: 10px;
    margin-left: 10px;
    width:100px;
    height:70px;
    border-radius: 5px;
    border:2px solid #FFFFFF;
    background-color: #63808F;
    display: flex;
    justify-content: center;
    align-items: center;
}

.nomer:hover{
    background-color: #63825F;
    cursor: pointer;
}

.nomer-chislo{
    font-family: 'Arimo', sans-serif;
    font-weight: 700;
    font-size: 50px;
    color:#FFFFFF;
}